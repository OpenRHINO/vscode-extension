{
  "name": "rhino",
  "displayName": "rhino",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenRHINO/vscode-extension.git"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rhino.create",
        "title": "Rhino: Create"
      },
      {
        "command": "rhino.build",
        "title": "Rhino: Build"
      },
      {
        "command": "rhino.run",
        "title": "Rhino: Run"
      },
      {
        "command": "rhino.logs",
        "title": "Rhino: Logs",
        "icon": "$(output)"
      },
      {
        "command": "rhino.jobs.refresh",
        "title": "Rhino: Refresh Job List",
        "icon": "$(refresh)"
      },
      {
        "command": "rhino.jobs.delete",
        "title": "Rhino: Delete Job",
        "icon": "$(trash)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rhino-sidebar",
          "title": "Rhino",
          "icon": "src/assets/icon.svg"
        }
      ]
    },
    "views": {
      "rhino-sidebar": [
        {
          "id": "rhino.jobs",
          "name": "Jobs"
        },
        {
          "id": "rhino.gpt",
          "name": "GPT"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "rhino.jobs.refresh",
          "when": "view == rhino.jobs",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "rhino.jobs.delete",
          "group": "inline",
          "when": "view == rhino.jobs && viewItem == jobItem"
        },
        {
          "command": "rhino.logs",
          "group": "inline",
          "when": "view == rhino.jobs && viewItem == jobItem"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "generator-code": "^1.7.6",
    "vsce": "^2.15.0",
    "yo": "^4.3.1"
  }
}
